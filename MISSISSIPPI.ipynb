    "def mississippi_fun(string : str):\n",
    "    out_dict = {}\n",
    "    for char in string:\n",
    "        if char in out_dict:\n",
    "            out_dict[char] += 1\n",
    "        else:\n",
    "            out_dict[char] = 1\n",
    "    return out_dict"
   ]
